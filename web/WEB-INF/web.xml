<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Musica_servlet</servlet-name>
        <servlet-class>control.Musica_servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Album_servlet</servlet-name>
        <servlet-class>control.Album_servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Artista_servlet</servlet-name>
        <servlet-class>control.Artista_servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login_servlet</servlet-name>
        <servlet-class>control.Login_servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Usuario_servlet</servlet-name>
        <servlet-class>control.Usuario_servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MusicaAvulsa_servlet</servlet-name>
        <servlet-class>control.MusicaAvulsa_servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Musica_servlet</servlet-name>
        <url-pattern>/sermu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Album_servlet</servlet-name>
        <url-pattern>/seral</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Artista_servlet</servlet-name>
        <url-pattern>/serar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login_servlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Usuario_servlet</servlet-name>
        <url-pattern>/serus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MusicaAvulsa_servlet</servlet-name>
        <url-pattern>/serma</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
